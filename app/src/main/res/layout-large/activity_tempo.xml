            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="TEMPO"
            android:textSize="35dp"
            android:layout_gravity="center"
            android:layout_marginLeft="10dp"
            android:textColor="@color/black"
            android:id="@+id/tmain"
