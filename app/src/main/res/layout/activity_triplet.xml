                    android:layout_width="120dp"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10dp"
                    android:text="Half Triplet"
                    android:textSize="18dp"
                    android:layout_gravity="center"
                    android:id="@+id/tvrn2"/>
